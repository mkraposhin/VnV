FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue T 300
    volScalarFieldValue p 1e5
    volVectorFieldValue U ( 10 0 0 )
    volScalarFieldValue volumeFraction1 0
    volScalarFieldValue Y.Liq 0
);

regions
(
    boxToCell
    {
        box ( -1 -1 -1 ) ( 5 1  1 );
        fieldValues
        (
            volScalarFieldValue volumeFraction1 1
            volScalarFieldValue Y.Liq 1
        );
    }
);


