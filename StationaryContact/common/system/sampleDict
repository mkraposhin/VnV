FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type sets;
libs ("libsampling.so");

interpolationScheme cell;

setFormat       csv;

sets
(
    line
    {
        type    uniform;
        axis    x;
        start   ( 0.00025 0 0 );
        end     ( 0.99975 0 0 );
        nPoints 2000;
    }
);

fields          ( volumeFraction1 p U T );


