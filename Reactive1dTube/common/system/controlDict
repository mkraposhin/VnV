FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "control.include"

tStartTime      0;
tSchlieren      0;
tEndTime        230e-6;

libs
(
    //libROUNDSchemes
);

startFrom       latestTime;

startTime       $tStartTime;

stopAt          endTime;

endTime         230e-6;

deltaT          1e-8;

writeControl    adjustableRunTime;

writeInterval   10e-6;

purgeWrite      0;

writeFormat     ascii;

writePrecision  12;

writeCompression on;

timeFormat      general;

timePrecision   12;

graphFormat     raw;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           0.1; //0.2; //为了对比，和 SMF 设置一样

maxDeltaT       1;

CourantType     cellCourant; //faceCourant

writeCourant    false;


